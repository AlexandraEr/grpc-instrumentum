    "url": "https://github.com/litichevskiydv/grpc-instrumentum/issues"
  },
  "homepage": "https://github.com/litichevskiydv/grpc-instrumentum#readme",
  "devDependencies": {
    "@grpc/proto-loader": "0.5.0",
    "grpc": "1.20.0",
    "jest": "24.7.1",
    "jest-extended": "0.11.1",
    "make-dir-cli": "2.0.0",
    "protobufjs": "6.8.8"
  },
  "dependencies": {
    "bl": "3.0.0",
    "camelcase": "5.3.1",
    "del": "4.1.0",
    "dot-prop": "5.0.0",
