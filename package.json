  "scripts": {
    "test": "jest --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/litichevskiydv/grpc-instrumentum.git"
  },
  "keywords": [
    "grpc",
    "protoc",
    "proto3"
  ],
  "author": "Dmitriy Litichevskiy",
  "license": "MIT",
